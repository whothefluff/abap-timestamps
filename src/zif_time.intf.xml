  <asx:values>
   <VSEOINTERF>
    <CLSNAME>ZIF_TIME</CLSNAME>
    <LANGU>E</LANGU>
    <DESCRIPT>Time</DESCRIPT>
    <EXPOSURE>2</EXPOSURE>
    <STATE>1</STATE>
    <UNICODE>X</UNICODE>
   </VSEOINTERF>
   <DESCRIPTIONS>
    <SEOCOMPOTX>
     <CMPNAME>ADD</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Adds values of provided arguments to this time</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>CHECK</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Throws an error if this time is not valid</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>EQUALS</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns true if both times have the same valid value</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>HOUR</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns the hour (0-23)</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>IS_EARLIER_THAN</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns true of the supplied time is ahead</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>IS_EARLIER_THAN_OR_EQUAL_TO</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns true of the supplied time is ahead or equal</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>IS_LATER_THAN</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns true of the supplied time is behind</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>IS_LATER_THAN_OR_EQUAL_TO</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns true of the supplied time is behind or equal</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>IS_VALID</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns &lt;em&gt;true&lt;/em&gt; if this time is valid</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>MINUTES_SINCE_MIDNIGHT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns the minutes since 00:00 (0-1440)</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>MINUTE_OF_HOUR</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns minute of the hour (0-59)</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SECONDS_SINCE_MIDNIGHT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns the seconds since 00:00 (0-86400)</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SECOND_OF_MINUTE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns second of the minute (0-59)</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SUBTRACT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Adds values of provided arguments to this time</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the specified format</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_0_TO_11_LOWER</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the 12-hour format hh:mm:ss am/pm</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_0_TO_11_UPPER</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the 12-hour format hh:mm:ss AM/PM</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_1_TO_12_LOWER</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the 12-hour format hh:mm:ss am/pm</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_1_TO_12_UPPER</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the 12-hour format hh:mm:ss AM/PM</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_24H</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the 24-hour form hh:mm:ss</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_DEFAULT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the default format</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_ISO_8601</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the ISO 8601 format</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_OF_COUNTRY</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the format of the specified country</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_OF_CURRENT_USER</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the format of the current user</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_FORMAT_OF_USER</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this time in the format of the specified user</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>T_VALUE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Internal type</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>VALID_VALUE_OR_ERROR</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns the value of this time if it&apos;s valid</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>VALID_VALUE_OR_FALLBACK</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns the value of this time if it&apos;s valid</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>VALUE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns the value of this time as is</DESCRIPT>
    </SEOCOMPOTX>
