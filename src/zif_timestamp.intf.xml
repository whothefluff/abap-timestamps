  <asx:values>
   <VSEOINTERF>
    <CLSNAME>ZIF_TIMESTAMP</CLSNAME>
    <LANGU>E</LANGU>
    <DESCRIPT>A time stamp of type p in form yyyymmddhhmmss.sssssss</DESCRIPT>
    <EXPOSURE>2</EXPOSURE>
    <STATE>1</STATE>
    <UNICODE>X</UNICODE>
   </VSEOINTERF>
   <DESCRIPTIONS>
    <SEOCOMPOTX>
     <CMPNAME>ADD</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Adds values of provided arguments to this time stamp</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>CHECK</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Throws an error if this time stamp is not valid</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>IS_DAYLIGHT_SAVING_TIME</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns &lt;em&gt;true&lt;/em&gt;when this time stamp is in summer time</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>IS_VALID</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns &lt;em&gt;true&lt;/em&gt; if this time stamp is valid</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SECONDS_OF_DIFFERENCE_WITH</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns time difference between this time stamp and other</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SHORT_VALUE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns this value as is without milliseconds</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SUBTRACT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Subtracts values of provided arguments to this time stamp</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_DATE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Creates a new date from this time stamp and returns it</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_DATE_FOR_DEFAULT_TZ</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Creates a new date from this time stamp and returns it</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>TO_DATE_FOR_SYSTEM_TZ</CMPNAME>
